{
    "editor.tabSize": 2,
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": "\\.bat",
                "cmd": "cd /d ${fileDirname} && call ${fileBasename}"
            },
            {
                "match": "\\.ahk",
                "cmd": "cd /d ${fileDirname} && start ${fileBasename}"
            }
        ]
    },
}